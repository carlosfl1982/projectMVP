    },
    "slider-layout#principal-slider-desk": {
        "props": {
            "itemsPerPage": {
                "desktop": 1,
                "tablet": 1,
                "phone": 1
            },
            "infinite": false,
            "blockClass": "carousel",
            "showNavigationArrows": "always",
            "showPaginationDots": "never",
            "fullWidth": true
        }
    },
    "flex-layout.col#subcategories-card-2": {
