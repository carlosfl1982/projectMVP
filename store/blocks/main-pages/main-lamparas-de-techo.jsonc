    "flex-layout.row#main-betaresults": {
        "children": [
            "search-result-layout.customQuery#main-pages"
        ],
        "props": {
            "blockClass": "main-cards",
            "preventHorizontalStretch": false
        }
    },
    "search-result-layout.customQuery#main-pages": {
        "props": {
            "querySchema": {
                "orderByField": "",
                "hideUnavailableItems": false,
                "maxItemsPerPage": 8,
                "queryField": "147",
                "mapField": "productClusterIds"
            }
        },
        "blocks": [
            "search-result-layout.desktop#prueba",
            "search-result-layout.mobile#prueba",
            "search-not-found-layout"
        ]
    },
    "search-result-layout.desktop#prueba": {
        "children": [
            // "breadcrumb.search",
            "search-title.v2",
            "flex-layout.row#top",
            "search-fetch-previous",
            "flex-layout.row#results",
            "search-fetch-more"
        ],
        "props": {
            "pagination": "show-more",
            "preventRouteChange": true,
            "mobileLayout": {
                "mode1": "small",
                "mode2": "normal"
              }
        }
    },
    "search-result-layout.mobile#prueba": {
        "children": [
            "search-title.v2",
            "flex-layout.row#top",
            "search-fetch-previous",
            "flex-layout.row#results",
            "search-fetch-more"
        ],
        "props": {
            "pagination": "show-more",
            "preventRouteChange": true,
            "mobileLayout": {
                "mode1": "small",
                "mode2": "normal"
              }
        }
    },
    "flex-layout.row#top": {
        "children": [
            "total-products.v2",
            "order-by.v2"
        ]
    },
    "flex-layout.row#results": {
        "children": [
