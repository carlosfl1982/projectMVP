{
    "store.custom#main-lamparas-de-techo": {
        "children": [
            "flex-layout.row#promotion-banner",
            "flex-layout.row#main-breadcrumb",
            "flex-layout.row#subcategories-cards",
            "flex-layout.row#main-destaques",
            "flex-layout.row#main-betaresults",
            "flex-layout.row#how-to"
        ],
        "props": {
            "blockClass": "department-home"
        }
    },
    "flex-layout.row#promotion-banner": {
        "children": [
            "flex-layout.col#promotion-banner"
        ],
        "props": {
            "blockClass": "main-prom-banner",
            "fullWidth": true
        }
    },
    "flex-layout.col#promotion-banner": {
        "children": [
            "list-context.image-list#promotion-banner"
        ],
        "props": {
            "blockClass": "main-prom-banner"
        }
    },
    "list-context.image-list#promotion-banner": {
        "props": {
            "blockClass": "main-prom-banner",
            "images": [
                {
                    "image": "/arquivos/banners3-4.jpg",
                    "mobileImage": ""
                }
            ]
        },
        "children": [
            "slider-layout#principal-slider-desk"
        ]
    },
    "flex-layout.row#main-breadcrumb": {
        "children": [
            "flex-layout.col#main-breadcrumb"
        ],
        "props": {
            "blockClass": "main-breadcrumb"
        }
    },
    "flex-layout.col#main-breadcrumb": {
        "children": [
            "rich-text#main-breadcrumb"
        ],
        "props": {
            "blockClass": "main-breadcrumb"
        }
    },
    "rich-text#main-breadcrumb": {
        "props": {
            "blockClass": "main-breadcrumb",
            "text": "LÃ¡mparas de techo &#8250;"
        }
    },
    "flex-layout.row#subcategories-cards": {
        "children": [
            "flex-layout.col#subcategories-card-1",
